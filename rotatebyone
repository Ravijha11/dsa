// <------------------------left by 1----------------->

let rotate = (arr,n) =>{
  let last = arr[0]
  for(let i=1; i<n;i++) {
    arr[i-1] = arr[i];



  }
  arr[n-1] = last;
  return arr;

}
console.log(rotate([15,545,5441,4445,8785,5546,556,555],8))